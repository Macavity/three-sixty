{
  "name": "three-sixty",
  "version": "1.0.0",
  "description": "A simple three sixty feedback tool",
  "main": "App.ts",
  "scripts": {
    "test": "jest",
    "dev:watch": "clasp push --watch",
    "gas:login": "clasp login",
    "csv:convert": "ts-node convert_csv.ts questions.csv > questions.ts",
    "lint": "eslint . --ext .ts --fix --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Macavity/three-sixty.git"
  },
  "keywords": [
    "three-sixty",
    "360 degree feedback",
    "peer feedback",
    "bias"
  ],
  "author": "ALRW",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Macavity/three-sixty/issues"
  },
  "homepage": "https://github.com/Macavity/three-sixty#readme",
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      [
        "@babel/preset-typescript",
        {
          "allowNamespaces": true
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@types/google-apps-script": "^1.0.64",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-jest": "^29.6.1",
    "csv-parse": "^5.4.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  }
}
